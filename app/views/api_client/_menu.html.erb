              <a href="/#/perfil" style="font-family: 'Titillium Web', sans-serif !important; font-size: 23px; text-decoration: none; color: #211651;">
                <%= user.name.capitalize + " " + user.last_name.capitalize %>
                <div class="v-avatar" style="height: 42px; width: 42px;">
                  <div aria-label="Avatar" class="inline v-image">
                    <% if user.image.attached? %> 
                      <%= image_tag(user_image, style:'object-fit: cover; border: 1px solid #1383cb') %> 
                    <% end%>
                  </div>
                </div>
                </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- MENU START -->
  <nav fixed style="background-color: #211651;">
    <% main_menus = General::Menu.where(parent_id: nil) %>
    <ul id="ul_menu" style="margin-bottom: 0px;">
      <% main_menus.each do |main_menu| %>
        <li class="submenu" id="submenu1">
          <a class="head_menu" style="padding-right: 100px; letter-spacing: 1px;" href="#" title="Menu 1">
            <%= main_menu.title.upcase %>
          </a>
          <ul class="megamenu" id="megamenu1">
            <% main_menu.children.each do |menu_parent| %>
              <% menu_parent_object = General::Menu.find(menu_parent[:menu_id]) %>
              <ul>
                <% if menu_parent_object.link.present? %>
                  <h4><%= link_to menu_parent_object.title, menu_parent_object.link, class:"head_section" %></h4>
                <% else %>
                  <h4 class="head_section"><%= menu_parent_object.title %></h4>
                <% end %>
                <% submenus = menu_parent_object.children(exa_menu) %>
                <% if submenus.length > 3 %>
                  <% submenus.first(3).each do |link| %>
                    <% benefit_count = 0 %>
                    <% if link[:link].downcase[0..3] == 'http'%>
                      <%= link_to link[:title], link[:link] %>
                    <% elsif link[:post_slug].present? %>
                      <%= link_to link[:title], '/#/noticias/' + link[:post_slug]%>
                    <% elsif main_menu.title == "Mis beneficios" %>
                      <% if benefits.present? %>
                        <% benefit = benefits.find_by_title(link[:title]) %>
                      <% end %>
                      <% if benefit.present? %>
                        <% benefit_count += 1 %>
                        <%= link_to link[:title], '/#/beneficios/' + benefit.id.to_s %>
                        <% if benefit_count > 2 %>
                          <%= link_to 'Ver más', '/#/beneficios', style: 'text-decoration: underline;' %>
                        <% end %>
                      <% end %>
                    <% else %>
                      <%= link_to link[:title], '/#/' + link[:link] %>
                    <% end %>
                  <% end %>
                  <!-- MENU MODAL START -->
                  <div class="modalDialog" id='menu_<%= menu_parent_object.title.downcase %>'>
                    <div class="text-md-center">
                      <a href="#" title="Close" class="close">X</a>
                      <div class="">
                        <% submenus.each do |link| %>
                        <div>
                          <% if link[:link].downcase[0..3] == 'http'%>
                            <%= link_to link[:title], link[:link] %><br>
                          <% elsif link[:post_slug].present? %>
                            <%= link_to link[:title], '/#/noticias/' + link[:post_slug]%>
                          <% else %>
                            <%= link_to link[:title], '/#/' + link[:link] %>
                          <% end %>
                        </div>
                        <% end %>
                      </div>
                    </div>
                  </div>
                  <!-- MENU MODAL END -->
                  <% if main_menu.title != "Mis beneficios" %>
                    <%= link_to 'Ver más', '/#menu_' + menu_parent_object.title.downcase, style: 'text-decoration: underline;' %>
                  <% end %>
                <% else %>
                  <% submenus.each do |link| %>
                    <% if link[:link].downcase[0..3] == 'http'%>
                      <%= link_to link[:title], link[:link] %>
                    <% elsif link[:post_slug].present? %>
                      <%= link_to link[:title], '/#/noticias/' + link[:post_slug]%>
                    <% else %>
                      <%= link_to link[:title], '/#/' + link[:link] %>
                    <% end %>
                  <% end %>
                <% end %>
              </ul>
            <% end %>
          </ul>
        </li>
      <% end %>
