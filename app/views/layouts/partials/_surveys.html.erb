<section class="section">
  <h5 class="section-header">Formularios</h5>
    <div class="card">
      <table class='table'>
        <thead>
          <tr>
            <th>Nombre</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @surveys.each do |survey| %>
            <tr>
              <td><%= survey.name %></td>
              <td><%= link_to t('.show', :default => t("helpers.links.show")),
            admin_survey_path(survey), :class => 'btn btn-primary btn-sm' %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-md-12 center">
      <%= will_paginate @surveys, renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
    </div>
</section>
