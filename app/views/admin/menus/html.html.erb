    <div class="v-app">
      <div class="hidden-sm-and-down">
        <div class="layout">
          <div class="flex sm1">
            <a href="/" >
              <img src="http://intranet-security-dev.s3-website.us-east-2.amazonaws.com/img/header.41ac21fa.png" style="height: 2rem; cursor: pointer;">
            </a>
          </div>
          <div class="flex sm2 text-md-center pt-2 subheading">
            <p>Santoral: <%= @data[:santoral].name %></p>
          </div>
          <div class="flex sm2 text-md-center pointer">
            <a href="#weather_modal" style="text-decoration: none; color: black;">
              <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].icon}.svg", style: " height: 24px;") %>
              <%= @data[:location_name] %>: 
              <% if @data[:weather][0].present? %>
                <%= @data[:weather][0].current_temp %>°
              <% else %>
                <%= 'Clima no disponible' %>
              <% end %>
            </a>
          </div>
          <div class="flex sm2 text-md-center subheading pointer">
            <a href="#indicators_modal" style="text-decoration: none; color: black;">
              <i aria-hidden="true" class="v-icon material-icons theme--light">
                multiline_chart</i>
              Indicadores
            </a>
          </div>
          <div class="flex text-md-right text-capitalize title font-weight-regular sm3">
            <a href="/perfil" style="text-decoration: none; color: black;">
              ¡Hola <%= @data[:user].name %>!
              <div class="v-avatar" style="height: 30px; width: 30px;">
                <div aria-label="Avatar" class="v-responsive v-image">
                  <% if @data[:user].image.attached? %>
                    <%= image_tag(@data[:user].image) %>
                  <% end %>
                </div>
              </div>
            </a>
          </div>
        </div>
        <div class="layout">
          <img src="data:image/png;base64," class="hidden-sm-and-down" style="height: 0.5rem; width: 50%;">
          <img src="data:image/png;base64," class="hidden-sm-and-down" style="height: 0.5rem; width: 50%;">
        </div>
      </div>
      <nav fixed>
        <ul id="ul_menu">
          <li class="submenu" id="submenu1">
            <a href="#" title="Menu 1" class="head_menu">EN LÍNEA</a>
            <ul class="megamenu" id="megamenu1">
              <% menu_parents = @data[:menus].where(id: [1, 5, 14, 17, 22, 25, 31, 35]) %>
              <% menu_parents.each do |menu_parent| %>
                <ul>
                  <h4 class="head_section"><%= menu_parent.title %></h4>
                  <% @data[:menus].where(parent_id: menu_parent.id).each do |link| %>
                    <% if link.link.downcase[0..3] == 'http'%>
                      <%= link_to link.title, link.link %>
                    <% else %>
                      <%= link_to link.title, '/' + link.link %>
                    <% end %>
                  <% end %>
                </ul>
              <% end %>
            </ul>
          </li>
          <li class="submenu" id="submenu1">
            <a href="#" title="Menu 1" class="head_menu">INFORMADOS</a>
            <ul class="megamenu" id="megamenu1">
              <% menu_parents = @data[:menus].where(id: [38, 42, 53, 56, 59, 65, 68, 71, 75, 78, 81, 82]) %>
              <% menu_parents.each do |menu_parent| %>
                <ul>
                  <h4 class="head_section"><%= menu_parent.title %></h4>
                  <% @data[:menus].where(parent_id: menu_parent.id).each do |link| %>
                    <% if link.link.downcase[0..3] == 'http'%>
                      <%= link_to link.title, link.link %>
                    <% else %>
                      <%= link_to link.title, '/' + link.link %>
                    <% end %>
                  <% end %>
                </ul>
              <% end %>
            </ul>
          </li>
          <li class="submenu" id="submenu1">
            <a href="#" title="Menu 1" class="head_menu">MIS BENEFICIOS</a>
            <ul class="megamenu" id="megamenu1">
              <% menu_parents = @data[:menus].where(id: [91, 105, 108, 113, 116, 125, 132, 149, 157, 164]) %>
              <% menu_parents.each do |menu_parent| %>
                <ul>
                  <h4 class="head_section"><%= menu_parent.title %></h4>
                  <% @data[:menus].where(parent_id: menu_parent.id).each do |link| %>
                    <% if link.link.downcase[0..3] == 'http'%>
                      <%= link_to link.title, link.link %>
                    <% else %>
                      <%= link_to link.title, '/' + link.link %>
                    <% end %>
                  <% end %>
                </ul>
              <% end %>
            </ul>
          </li>
        </ul>
      </nav>
      <div id="weather_modal" class="modalDialog">
        <div>
          <a href="#close" title="Close" class="close">X</a>
          <div class="layout">
            <div class="flex">
              <div class="v-card v-card--flat v-sheet theme--light">
                <div class="v-responsive v-image" style="height: 100px;">
                  <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].icon}.svg", class: "v-image__image v-image__image--contain", style: "background-position: center center;") %>
                </div>
                <div class="v-card__title">
                  <div class="flex">
                    <h3 class="text-md-center headline mb-2">Hoy</h3>
                    <p class="text-md-center subheading">Actual: <%= @data[:weather][0].current_temp %>°</p>
                    <p class="text-md-center subheading">Mínima:<%= @data[:weather][0].min_temp %>°</p>
                    <p class="text-md-center subheading">Máxima:<%= @data[:weather][0].max_temp %>°</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="flex">
              <div class="v-card v-card--flat v-sheet theme--light">
                <div class="v-responsive v-image" style="height: 100px;">
                  <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].tomorrow_icon}.svg", class: "v-image__image v-image__image--contain", style: "background-position: center center;") %>
                </div>
                <div class="v-card__title">
                  <div class="flex">
                    <h3 class="text-md-center headline mb-2"> <%= l(Date.today + 1, format: '%A') %></h3>
                    <p class="text-md-center subheading">Mínima:<%= @data[:weather][0].tomorrow_max %>°</p>
                    <p class="text-md-center subheading">Máxima:<%= @data[:weather][0].tomorrow_min %>°</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="flex">
              <div class="v-card v-card--flat v-sheet theme--light">
                <div class="v-responsive v-image" style="height: 100px;">
                  <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].after_tomorrow_icon}.svg", class: "v-image__image v-image__image--contain", style: "background-position: center center;") %>
                </div>
                <div class="v-card__title">
                  <div class="flex">
                    <h3 class="text-md-center headline mb-2"> <%= l(Date.today + 2, format: '%A') %></h3>
                    <p class="text-md-center subheading">Mínima:<%= @data[:weather][0].after_tomorrow_min %>°</p>
                    <p class="text-md-center subheading">Máxima:<%= @data[:weather][0].after_tomorrow_max %>°</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="flex">
              <div class="v-card v-card--flat v-sheet theme--light">
                <div class="v-responsive v-image" style="height: 100px;">
                  <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].aa_tomorrow_icon}.svg", class: "v-image__image v-image__image--contain", style: "background-position: center center;") %>
                </div>
                <div class="v-card__title">
                  <div class="flex">
                    <h3 class="text-md-center headline mb-2"> <%= l(Date.today + 3, format: '%A') %></h3>
                    <p class="text-md-center subheading">Mínima:<%= @data[:weather][0].aa_tomorrow_min %>°</p>
                    <p class="text-md-center subheading">Máxima:<%= @data[:weather][0].aa_tomorrow_max %>°</p>
                  </div>
                </div>
              </div>
            </div>
            <div class="flex">
              <div class="v-card v-card--flat v-sheet theme--light">
                <div class="v-responsive v-image" style="height: 100px;">
                  <%= image_tag("#{root_url}assets/weather/#{@data[:weather][0].aaa_tomorrow_icon}.svg", class: "v-image__image v-image__image--contain", style: "background-position: center center;") %>
                </div>
                <div class="v-card__title">
