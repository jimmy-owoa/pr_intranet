                    <p class="text-md-center subheading">Mínima:<%= @data[:weather][0].aaa_tomorrow_min %>°</p>
                    <p class="text-md-center subheading">Máxima:<%= @data[:weather][0].aaa_tomorrow_max %>°</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
