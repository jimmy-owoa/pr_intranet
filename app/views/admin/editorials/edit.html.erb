<%- model_class = Library::Editorial -%>
<section class="section">
  <h1 class="section-header">Editar Editorial: <%= @editorial.name %></h1>
  <%= simple_form_for @editorial, as: 'editorial', url: {action: :update}, :html => { :class => "form" } do |f| %>
    <%= render :partial => 'form', locals: {f: f} %>
  <% end %>
</section>