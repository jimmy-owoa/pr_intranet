<%= f.error_notification %>
<div class="card">
  <div class="card-body">
    <div class="form-inputs">
      <div class="row">
        <div class="col-md-12 form-group">
            <%= file_field_tag 'attachment[attachment][]', id: "fileinputuploader", class: 'file-loading', multiple: true %>
        </div>
        <div class="col-4 col-sm-5">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <%= show_media @attachment if params['action'] == 'edit' %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <br />
  </div>
</div>


