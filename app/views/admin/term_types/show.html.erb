<h1> Tipo de taxonomía id: <%= @term_type.id %></h1>

<%= link_to t('.back', :default => t("helpers.links.back")),
              admin_term_types_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_term_type_path(@term_type), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              admin_term_type_path(@term_type),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => '¿Esta segura(o) que desea eliminar este elemento?')) },
              :class => 'btn btn-danger' %>
