<div class="container">
  <div class="row title_form">
    <div class="col-sm-12">
      <div class="card">
        <div class="card-header" align="center">
          <div>
            <h2> <strong> <%= @survey.name %> </strong> </h2>
          </div>
        </div>
        <div class="card-body">
          <p class="card-text">
            <div class="table-responsive">
              <table class="table">
                <thead>
                  <tr>
                    <th scope="col">User id</th>
                    <th scope="col">Detalle</th>
                  </tr>
                </thead>
                <tbody>
                  <% @survey.questions.group_by(&:survey_id).each do |question| %>
                    <tr>
                      <td><%= question[1][0].answers.first.user_id %></td>
                      <td><%=  %></td>
                    </tr>
                  <% end %>
                </tbody>  
              </table>
            </div>
          </p>
      </div>
    </div>
  </div>
</div>
